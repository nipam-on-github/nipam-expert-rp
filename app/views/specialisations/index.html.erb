<p id="notice"><%= notice %></p>

<h1>Specialisations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @specialisations.each do |specialisation| %>
      <tr>
        <td><%= link_to 'Show', specialisation %></td>
        <td><%= link_to 'Edit', edit_specialisation_path(specialisation) %></td>
        <td><%= link_to 'Destroy', specialisation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Specialisation', new_specialisation_path %>
