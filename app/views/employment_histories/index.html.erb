<p id="notice"><%= notice %></p>

<h1>Employment Histories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employment_histories.each do |employment_history| %>
      <tr>
        <td><%= link_to 'Show', employment_history %></td>
        <td><%= link_to 'Edit', edit_employment_history_path(employment_history) %></td>
        <td><%= link_to 'Destroy', employment_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employment History', new_employment_history_path %>
